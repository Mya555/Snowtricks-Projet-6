<ul class="list_ul" id="video-fields-list"
    data-prototype="{{ form_widget(form.mediaVideos.vars.prototype)|e}}"
    data-widget-tags="{{ '<li></li>'|e }}">

    {% for video in form.mediaVideos %}
        <div {% if video.vars.data.id is not empty %} class="hidden">{% endif %}
            {{ form_errors(video) }}
            {{ form_widget(video) }}
        </div>
    {% endfor %}
</ul>
<div class="col-sm-7">
    <a href="#" class=" btn btn-info add-another-collection-widget_video"
       data-list="#video-fields-list">Ajouter une video</a>
</div>

