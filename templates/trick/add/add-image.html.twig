<ul class="list_ul" id="image-fields-list"
    data-prototype="{{ form_widget(form.images.vars.prototype)|e('html_attr') }}"
    data-widget-tags="{{ '<li></li>'|e }}">

{% for image in form.images %}
   <li>
       <div {% if image.vars.data.id is not empty %} class="hidden">{% endif %}
        {{ form_errors(image) }}
        {{ form_widget(image) }}
        </div>
   </li>
{% endfor %}
</ul>
<div class="col-sm-7">
    <a href="#" class=" image_btn btn btn-info add-another-collection-widget"
       data-list="#image-fields-list">Ajouter une image
    </a><br>
</div>
